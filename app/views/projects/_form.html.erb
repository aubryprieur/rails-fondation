
<%= simple_form_for @project do |f| %>

  <%= f.input :title %>
  <%= f.input :description, input_html: {style: 'height:20vh;'}  %>
  <%= f.input :expiration%>
  <%= f.input :link %>
  <%= f.association :fondation %>
  <%= f.association :category %>
  <%= f.input :all_tags, placeholder: "Tags séparées par une virgule" %>
  <%= f.button :submit %>
<% end %>
